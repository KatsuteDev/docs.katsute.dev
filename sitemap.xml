---
layout: compress
---
{% assign __mod = site.time | date: "%Y-%m-%d" %}
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="https://www.sitemaps.org/schemas/sitemap/0.9">
    {% for __page in site.pages %}
        {% assign __head = __page.path | split: '/' %}
        {% if
            __head[0] != "404.html" and
            __head[0] != "dependencies"
        %}
            <url>
                <loc>{{ site.url }}/{{ __page.path | remove: "index.html" | remove: "README.md" | remove: ".md" | remove: ".html" }}</loc>
                <lastmod>{{ __mod }}</lastmod>
                <changefreq>weekly</changefreq>
                <priority>0.5</priority>
            </url>
        {% endif %}
    {% endfor %}
</urlset>